# Supabase Configuration (Required for authentication and storage)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Flutterwave Configuration (Required for payments)
NEXT_PUBLIC_FLUTTERWAVE_PUBLIC_KEY=FLWPUBK-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-X
FLUTTERWAVE_SECRET_KEY=FLWSECK-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-xxxxxxxxxxxxvt-X

# AI Configuration (DeepSeek API - Required for AI geometry generation)
# Uses direct API calls to https://api.deepseek.com/v1/chat/completions
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here

# App URL (Required for redirects and links)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase Storage S3 (server-side; required for uploads/downloads)
SUPABASE_S3_ENDPOINT=https://your-project.storage.supabase.co/storage/v1/s3
SUPABASE_S3_REGION=your-region
SUPABASE_S3_ACCESS_KEY_ID=your-s3-access-key
SUPABASE_S3_SECRET_ACCESS_KEY=your-s3-secret-key

# Optional: Database (if using direct PostgreSQL connection)
# DATABASE_URL=postgresql://user:password@localhost:5432/qutlas

# Optional: Analytics
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your-analytics-id
